{
  "name": "1 - Pump Control",
  "script_path": "mpumptimed.py",
  "description": "Turns on specific pump(s) for a specified amount of time",
  "working_directory": "/eve/scripts/webui-sc",
  "parameters": [
    {
      "name": "EVE #",
      "default": "1"
    },
    {
      "name": "Pump",
      "type": "multiselect",
      "required": true,
      "values": [
        "Drug",
        "Media",
        "Waste"
      ],
      "default": [
          "Drug",
          "Media",
          "Waste"
      ]
    },
    {
      "name": "Pump Duration (s)",
      "default": "60"
    }
  ]
}
